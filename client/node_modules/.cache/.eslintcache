 category ID from URL parameters\r\n  const catId = parseInt(useParams().id);\r\n\r\n  // State for filtering by maximum price and sorting\r\n  const [maxPrice, setMaxPrice] = useState(1000);\r\n  const [sort, setSort] = useState(null);\r\n\r\n  // State for tracking selected sub-categories\r\n  const [selectedSubCats, setSelectedSubCats] = useState([]);\r\n\r\n  // Fetch sub-categories for the selected category\r\n  const { data, loading, error } = useFetch(\r\n    `${process.env.REACT_APP_API_URL}/sub-categories?[filters][categories][id][$eq]=${catId}`\r\n  );\r\n  console.log(\"API URL:\", process.env.REACT_APP_API_URL);\r\n\r\n  // Handle change in selected sub-categories\r\n  const handleChange = (e) => {\r\n    const value = e.target.value;\r\n    const isChecked = e.target.checked;\r\n\r\n    setSelectedSubCats(\r\n      isChecked\r\n        ? [...selectedSubCats, value]\r\n        : selectedSubCats.filter((item) => item !== value)\r\n    );\r\n  };\r\n\r\n  return (\r\n    <div className=\"products\">\r\n      {/* Left section with filters */}\r\n      <div className=\"left\">\r\n        {/* Filter by sub-categories */}\r\n        <div className=\"filterItem\">\r\n          <h2>Product Categories</h2>\r\n          {data?.map((item) => (\r\n            <div className=\"inputItem\" key={item.id}>\r\n              <input\r\n                type=\"checkbox\"\r\n                id={item.id}\r\n                value={item.id}\r\n                onChange={handleChange}\r\n              />\r\n              <label htmlFor={item.id}>{item.attributes.title}</label>\r\n            </div>\r\n          ))}\r\n        </div>\r\n        {/* Filter by price */}\r\n        <div className=\"filterItem\">\r\n          <h2>Filter by price</h2>\r\n          <div className=\"inputItem\">\r\n            <span>0</span>\r\n            <input\r\n              type=\"range\"\r\n              min={0}\r\n              max={1000}\r\n              onChange={(e) => setMaxPrice(e.target.value)}\r\n            />\r\n            <span>{maxPrice}</span>\r\n          </div>\r\n        </div>\r\n        {/* Sort options */}\r\n        <div className=\"filterItem\">\r\n          <h2>Sort by</h2>\r\n          <div className=\"inputItem\">\r\n            <input\r\n              type=\"radio\"\r\n              id=\"asc\"\r\n              value=\"asc\"\r\n              name=\"price\"\r\n              onChange={(e) => setSort(\"asc\")}\r\n            />\r\n            <label htmlFor=\"asc\">Price (Lowest first)</label>\r\n          </div>\r\n          <div className=\"inputItem\">\r\n            <input\r\n              type=\"radio\"\r\n              id=\"desc\"\r\n              value=\"desc\"\r\n              name=\"price\"\r\n              onChange={(e) => setSort(\"desc\")}\r\n            />\r\n            <label htmlFor=\"desc\">Price (Highest first)</label>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      {/* Right section with product list */}\r\n      <div className=\"right\">\r\n        {/* Display category image */}\r\n        <img\r\n          className=\"catImg\"\r\n          src=\"https://static.zarahome.net/8/photos4/2023/I/4/1/b/4226/000/737/BH/ZZ/4226000737_12_2_1.jpg?t=1690985356935&imformat=chrome\"\r\n          alt=\"\"\r\n        />\r\n        {/* Render product list */}\r\n        <List catId={catId} maxPrice={maxPrice} sort={sort} subCats={selectedSubCats} />\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Products;\r\n","C:\\Users\\Marcia\\OneDrive\\Escritorio\\Web-dev\\Working\\Ecommerce-2023\\deco-shop\\client\\src\\components\\Slider\\slider.jsx",[],[],"C:\\Users\\Marcia\\OneDrive\\Escritorio\\Web-dev\\Working\\Ecommerce-2023\\deco-shop\\client\\src\\components\\Categories\\Categories.jsx",[],[],"C:\\Users\\Marcia\\OneDrive\\Escritorio\\Web-dev\\Working\\Ecommerce-2023\\deco-shop\\client\\src\\components\\FeaturedProducts\\FeaturedProducts.jsx",[],[],"C:\\Users\\Marcia\\OneDrive\\Escritorio\\Web-dev\\Workin