      max={1000}\r\n              onChange={(e) => setMaxPrice(e.target.value)}\r\n            />\r\n            <span>{maxPrice}</span>\r\n          </div>\r\n        </div>\r\n        {/* Sort options */}\r\n        <div className=\"filterItem\">\r\n          <h2>Sort by</h2>\r\n          <div className=\"inputItem\">\r\n            <input\r\n              type=\"radio\"\r\n              id=\"asc\"\r\n              value=\"asc\"\r\n              name=\"price\"\r\n              onChange={(e) => setSort(\"asc\")}\r\n            />\r\n            <label htmlFor=\"asc\">Price (Lowest first)</label>\r\n          </div>\r\n          <div className=\"inputItem\">\r\n            <input\r\n              type=\"radio\"\r\n              id=\"desc\"\r\n              value=\"desc\"\r\n              name=\"price\"\r\n              onChange={(e) => setSort(\"desc\")}\r\n            />\r\n            <label htmlFor=\"desc\">Price (Highest first)</label>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      {/* Right section with product list */}\r\n      <div className=\"right\">\r\n        {/* Display category image */}\r\n        <img\r\n          className=\"catImg\"\r\n          src=\"https://static.zarahome.net/8/photos4/2023/I/4/1/b/4226/000/737/BH/ZZ/4226000737_12_2_1.jpg?t=1690985356935&imformat=chrome\"\r\n          alt=\"\"\r\n        />\r\n        {/* Render product list */}\r\n        <List catId={catId} maxPrice={maxPrice} sort={sort} subCats={selectedSubCats} />\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Products;\r\n","C:\\Users\\Marcia\\OneDrive\\Escritorio\\Web-dev\\Working\\Ecommerce-2023\\deco-shop\\client\\src\\components\\Slider\\slider.jsx",[],[],"C:\\Users\\Marcia\\OneDrive\\Escritorio\\Web-dev\\Working\\Ecommerce-2023\\deco-shop\\client\\src\\components\\Categories\\Categories.jsx",[],[],"C:\\Users\\Marcia\\OneDrive\\Escritorio\\Web-dev\\Working\\Ecommerce-2023\\deco-shop\\client\\src\\components\\FeaturedProducts\\FeaturedP